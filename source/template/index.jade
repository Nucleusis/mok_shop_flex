html
    head
        meta(charset="utf-8")
        title Магазин
        link(rel="stylesheet" type="text/css" href="css/style.css")
    body
        header.main-header
            header.navbar
                nav.main-nav
                    a(href="#") О Магазине
                    a(href="#") Акции
                    a(href="#") Доставка и Оплата
                    a(href="#") Помощь
                    a(href="#") Контакты
                div.nav-user
                    div.drop-down-menu
                        a(href="#").shop-menu Мой магазин
                        div.drop-down-content
                            a(href="#") Мои заказы
                            a(href="#") Мои акции
                            a(href="#") Мой профиль
                    a(href="#" class="enter") Вход
            section
                img(src="" alt="logo" id="logo")
                article
                    div.header-content
                        div.select-place
                            select
                                option(selected) Днепропетровск
                                option Киев
                                option Львов
                                option Харьков
                        div.header-contacts
                            div
                                a(href="#") Заказать звонок
                                p.phone 0 800 555 00 55

                            div
                                a(href="#") on-line чат
                                p.phone 044 555 00 55
                        div#header-cart
                            a(href="#") Корзина
                    form.header-search
                        input(type="text")
                        div.select-search
                            select
                                option(selected) Во всем каталоге
                                option В наличии
                                option
                        input(type="submit" value="")
        section
            nav
                img(src="" alt="back")
                a(href="#") Назад
                p Главная &gt; Каталог товаров &gt; Компьютерная техника
            article
                img(src="" alt="slider")
            article
                header
                    h3 Выбор по бренду
                    a(href="#") все бренды
                p
                    - for (var i = 0; i < 7*3; i++)
                        img(src="" alt="img")
            article
                header
                    h3 Акционные предложения
                    a(href="#") все акции
                p
                    img(src="" alt="previous")
                    - for (var i = 0; i < 5; i++)
                        div
                            img(src="" alt="img")
                            p text
                    img(src="" alt="next")
                footer
                    p 1 - 5 из 21
            article
                header
                    h3 Обзоры
                    a(href="#") все обзоры
                p
                    - for (var i = 0; i < 3; i++)
                        div
                            img(src="" alt="img")
                            p text
                            p time
                                img(src="" alt="rate")